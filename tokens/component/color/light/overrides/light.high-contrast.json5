/**
 * Light high contrast
 * @extends all functional tokens
 * @description All overwrites for functional color tokens for light high contrast color mode are in this file
 */
{
  comp: {
    color: {
      switchTrack: {
        bg: {
          $value: '{ref.color.scale.white}',
          $type: 'color'
        },
        border: {
          $value: '{sys.color.border.default}',
          $type: 'color'
        },
        checked: {
          border: {
            $value: '{ref.color.scale.blue.5}',
            $type: 'color'
          }
        }
      }
    }
  }
}
