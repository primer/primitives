{
  "borderInset": {
    "thin": {
      "value": "inset 0 0 0 {borderWidth.thin}",
      "$type": "size"
    },
    "thick": {
      "value": "inset 0 0 0 {borderWidth.thick}",
      "$type": "size"
    },
    "thicker": {
      "value": "inset 0 0 0 {borderWidth.thicker}",
      "$type": "size"
    }
  },
  "borderWidth": {
    "thin": {
      "value": "max(1px, 0.0625rem)",
      "$type": "size"
    },
    "thick": {
      "value": "max(2px, 0.125rem)",
      "$type": "size"
    },
    "thicker": {
      "value": "max(4px, 0.25rem)",
      "$type": "size"
    }
  },
  "borderRadius": {
    "small": {
      "value": "3px",
      "$type": "size"
    },
    "medium": {
      "value": "6px",
      "$type": "size"
    },
    "large": {
      "value": "12px",
      "$type": "size"
    },
    "full": {
      "value": "100vh",
      "$type": "size"
    }
  }
}
