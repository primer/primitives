        "$value": "{base.color.scale.blue.4}",
        "$type": "color",
        "alpha": 0.4
      }
    },
    "diffBlob": {
      "addition": {
        "fg": {
          "$value": "{color.fg.onEmphasis}",
          "$type": "color"
        }
      },
      "deletion": {
        "fg": {
          "$value": "{color.fg.onEmphasis}",
          "$type": "color"
        }
      },
      "expander": {
        "icon": {
          "$value": "{color.fg.default}",
          "$type": "color"
        }
      },
      "hunk": {
        "numBg": {
          "$value": "{color.accent.muted}",
          "$type": "color",
          "alpha": 0.4
        }
      }
    },

    "actionListItem": {
      "inlineDivider": {
        "$value": "{color.border.muted}",
        "$type": "color"
      },

      "default": {
        "hoverBg": {
          "$value": "{base.color.scale.gray.8}",
          "$type": "color"
        },
        "hoverBorder": {
          "$value": "{color.border.default}",
          "$type": "color"
        },
        "activeBg": {
          "$value": "{base.color.scale.gray.6}",
          "$type": "color"
        },
        "activeBorder": {
          "$value": "{base.color.scale.gray.4}",
          "$type": "color"
        },
        "selectedBg": {
          "$value": "{base.color.scale.gray.6}",
          "$type": "color"
        }
      },
      "danger": {
        "hoverBg": {
          "$value": "{color.danger.emphasis}",
          "$type": "color"
        },
        "activeBg": {
          "$value": "{base.color.scale.red.6}",
          "$type": "color"
        },
        "hoverText": {
          "$value": "{color.fg.onEmphasis}",
          "$type": "color"
        }
      }
    },

    "switchTrack": {
      "border": {
        "$value": "{color.border.default}",
        "$type": "color"
      },
      "checked": {
        "border": {
          "$value": "{base.color.scale.blue.5}",
          "$type": "color"
        }
