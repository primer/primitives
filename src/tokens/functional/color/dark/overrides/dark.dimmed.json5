/**
 * Dark dimmed
 * @extends all functional tokens
 * @description All overwrites for functional color tokens for dark dimmed color mode are in this file
 */
{
  bgColor: {
    default: {
      $value: '{base.color.neutral.3}',
      $type: 'color',
    },
    muted: {
      $value: '{base.color.neutral.4}',
      $type: 'color',
    },
    inset: {
      $value: '{base.color.neutral.2}',
      $type: 'color',
    },
    disabled: {
      $value: '{base.color.neutral.5}',
      $type: 'color',
    },
    neutral: {
      emphasis: {
        $value: '{base.color.neutral.8}',
        $type: 'color',
      },
    },
  },
  fgColor: {
    default: {
      $value: '{base.color.neutral.11}',
      $type: 'color',
    },
    onEmphasis: {
      $value: '{base.color.neutral.12}',
      $type: 'color',
    },
    disabled: {
      $value: '{base.color.neutral.8}',
      $type: 'color',
      alpha: 1,
    },
  },
  control: {
    bgColor: {
      rest: {
        $value: '{base.color.neutral.5}',
        $type: 'color',
      },
      hover: {
        $value: '{base.color.neutral.6}',
        $type: 'color',
      },
      active: {
        $value: '{base.color.neutral.7}',
        $type: 'color',
      },
    },
    borderColor: {
      rest: {
        $value: '{borderColor.default}',
        $type: 'color',
      },
    },
    transparent: {
      bgColor: {
        hover: {
          $value: '{base.color.neutral.8}',
          $type: 'color',
          alpha: 0.15,
        },
        active: {
          $value: '{base.color.neutral.8}',
          $type: 'color',
          alpha: 0.2,
        },
        selected: {
          $value: '{base.color.neutral.8}',
          $type: 'color',
          alpha: 0.1,
        },
      },
    },
  },
  controlTrack: {
    bgColor: {
      rest: {
        $value: '{base.color.neutral.5}',
        $type: 'color',
      },
      hover: {
        $value: '{base.color.neutral.6}',
        $type: 'color',
      },
      active: {
        $value: '{base.color.neutral.7}',
        $type: 'color',
      },
    },
  },
  controlKnob: {
    bgColor: {
      rest: {
        $value: '{bgColor.inset}',
        $type: 'color',
        $extensions: {
          'org.primer.figma': {
            collection: 'mode',
            mode: 'dark',
            group: 'component (internal)',
            scopes: ['bgColor'],
          },
        },
      },
    },
  },
  diffBlob: {
    expander: {
      iconColor: {
        $value: '{fgColor.default}',
        $type: 'color',
      },
    },
  },
  overlay: {
    backdrop: {
      bgColor: {
        $value: '{base.color.neutral.4}',
        $type: 'color',
        alpha: 0.4,
      },
    },
    bgColor: {
      $value: '{base.color.neutral.5}',
      $type: 'color',
    },
  },
  button: {
    primary: {
      bgColor: {
        disabled: {
          $value: '{base.color.green.4}',
          $type: 'color',
          mix: {
            color: '{base.color.green.3}',
            weight: 0.6,
          },
        },
      },
      fgColor: {
        rest: {
          $value: '#ffffff',
          $type: 'color',
        },
      },
      iconColor: {
        rest: {
          $value: '#ffffff',
          $type: 'color',
        },
      },
    },
    invisible: {
      iconColor: {
        rest: {
          $value: '{base.color.neutral.10}',
          $type: 'color',
        },
        hover: {
          $value: '{base.color.neutral.10}',
          $type: 'color',
        },
      },
    },
    inactive: {
      bgColor: {
        $value: '{base.color.neutral.5}',
        $type: 'color',
      },
      fgColor: {
        $value: '{base.color.neutral.8}',
        $type: 'color',
      },
    },
  },
  timelineBadge: {
    bgColor: {
      $value: '{base.color.neutral.4}',
      $type: 'color',
    },
  },
}
