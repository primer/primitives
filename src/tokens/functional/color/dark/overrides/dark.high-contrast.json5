/**
 * Dark high contrast
 * @extends all functional tokens
 * @description All overwrites for functional color tokens for dark high contrast color mode are in this file
 */
{
  fgColor: {
    onEmphasis: {
      $value: '{base.color.gray.9}',
      $type: 'color',
    },
    muted: {
      $value: '{base.color.gray.1}',
      $type: 'color',
    },
  },
  bgColor: {
    emphasis: {
      $value: '{base.color.gray.0}',
      $type: 'color',
    },
    neutral: {
      emphasis: {
        $value: '{base.color.gray.4}',
        $type: 'color',
      },
    },
  },
  borderColor: {
    default: {
      $value: '{base.color.gray.5}',
      $type: 'color',
    },
    muted: {
      $value: '{base.color.gray.5}',
      $type: 'color',
    },
    decorative: {
      $value: '{borderColor.default}',
      $type: 'color',
    },
    neutral: {
      emphasis: {
        $value: '{base.color.gray.4}',
        $type: 'color',
      },
    },
    accent: {
      muted: {
        $value: '{base.color.blue.4}',
        $type: 'color',
      },
    },
    success: {
      muted: {
        $value: '{base.color.green.4}',
        $type: 'color',
      },
    },
    attention: {
      muted: {
        $value: '{base.color.yellow.4}',
        $type: 'color',
      },
    },
    severe: {
      muted: {
        $value: '{base.color.orange.4}',
        $type: 'color',
      },
    },
    danger: {
      muted: {
        $value: '{base.color.red.4}',
        $type: 'color',
      },
    },
    done: {
      muted: {
        $value: '{base.color.purple.4}',
        $type: 'color',
      },
    },
    sponsors: {
      muted: {
        $value: '{base.color.pink.4}',
        $type: 'color',
      },
    },
  },
  // patterns
  avatar: {
    borderColor: {
      $value: '{base.color.white}',
      $type: 'color',
      alpha: 0.9,
    },
  },
  topicTag: {
    borderColor: {
      $value: '{borderColor.accent.emphasis}',
      $type: 'color',
    },
  },
  control: {
    bgColor: {
      selected: {
        $value: '{base.color.gray.6}',
        $type: 'color',
      },
    },
    borderColor: {
      emphasis: {
        $value: '{borderColor.emphasis}',
        $type: 'color',
      },
    },
    transparent: {
      bgColor: {
        hover: {
          $value: '{base.color.gray.8}',
          $type: 'color',
        },
        active: {
          $value: '{base.color.gray.6}',
          $type: 'color',
        },
        selected: {
          $value: '{base.color.gray.6}',
          $type: 'color',
        },
      },
      borderColor: {
        hover: {
          $value: '{borderColor.default}',
          $type: 'color',
        },
        active: {
          $value: '{base.color.gray.4}',
          $type: 'color',
        },
      },
    },
    danger: {
      fgColor: {
        hover: {
          $value: '{fgColor.onEmphasis}',
          $type: 'color',
        },
      },
      bgColor: {
        hover: {
          $value: '{bgColor.danger.emphasis}',
          $type: 'color',
        },
        active: {
          $value: '{bgColor.danger.emphasis}',
          $type: 'color',
          mix: {
            color: '{base.color.red.7}',
            weight: 0.4,
          },
        },
      },
    },
  },
  controlTrack: {
    fgColor: {
      rest: {
        $value: '{fgColor.onEmphasis}',
        $type: 'color',
      },
    },
  },
  button: {
    primary: {
      fgColor: {
        rest: {
          $value: '{fgColor.onEmphasis}',
          $type: 'color',
        },
        disabled: {
          $value: '{fgColor.onEmphasis}',
          $type: 'color',
          alpha: 0.5,
        },
        iconColor: {
          rest: {
            $value: '{fgColor.onEmphasis}',
            $type: 'color',
          },
        },
      },
      bgColor: {
        disabled: {
          $value: '{base.color.green.5}',
          $type: 'color',
          mix: {
            color: '{base.color.green.0}',
            weight: 0.6,
          },
        },
      },
      borderColor: {
        rest: {
          $value: '{base.color.green.2}',
          $type: 'color',
        },
        hover: {
          $value: '{base.color.green.2}',
          $type: 'color',
        },
        disabled: {
          $value: '{base.color.green.2}',
          $type: 'color',
          alpha: 0.4,
        },
      },
    },
  },
  buttonCounter: {
    primary: {
      bgColor: {
        rest: {
          $value: '{base.color.black}',
          $type: 'color',
          alpha: 0.15,
        },
      },
    },
    danger: {
      bgColor: {
        hover: {
          $value: '{base.color.black}',
          $type: 'color',
          alpha: 0.15,
        },
        rest: {
          $value: '{base.color.black}',
          $type: 'color',
          alpha: 0.15,
        },
      },
    },
  },
  diffBlob: {
    addition: {
      fgColor: {
        text: {
          $value: '{fgColor.onEmphasis}',
          $type: 'color',
        },
      },
      bgColor: {
        word: {
          $value: '{base.color.green.4}',
          $type: 'color',
          alpha: 1,
        },
      },
    },
    deletion: {
      fgColor: {
        text: {
          $value: '{fgColor.onEmphasis}',
          $type: 'color',
        },
      },
      bgColor: {
        word: {
          $value: '{base.color.red.5}',
          $type: 'color',
          alpha: 1,
        },
      },
    },
    expander: {
      iconColor: {
        $value: '{fgColor.default}',
        $type: 'color',
      },
    },
    hunk: {
      bgColor: {
        num: {
          $value: '{base.color.blue.4}',
          $type: 'color',
          alpha: 0.4,
        },
      },
    },
  },
  underlineNav: {
    borderColor: {
      hover: {
        $value: '{base.color.gray.3}',
        $type: 'color',
      },
    },
    iconColor: {
      rest: {
        $value: '{base.color.gray.1}',
        $type: 'color',
      },
    },
  },
  codeMirror: {
    selection: {
      bgColor: {
        $value: '{base.color.blue.4}',
        $type: 'color',
        alpha: 0.4,
      },
    },
  },
}
