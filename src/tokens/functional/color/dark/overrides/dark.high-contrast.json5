/**
 * Dark high contrast
 * @extends all functional tokens
 * @description All overwrites for functional color tokens for dark high contrast color mode are in this file
 */
{
  "color": {
    "fg": {
      "muted": {
        "$value": "{base.color.gray.1}",
        "$type": "color"
      },
      "onEmphasis": {
        "$value": "{base.color.gray.9}",
        "$type": "color"
      }
    },
    "border": {
      "default": {
        "$value": "{base.color.gray.5}",
        "$type": "color"
      },
      "muted": {
        "$value": "{base.color.gray.5}",
        "$type": "color"
      },
      "subtle": {
        "$value": "{base.color.gray.5}",
        "$type": "color"
      }
    },
    "neutral": {
      "emphasis": {
        "$value": "{base.color.gray.4}",
        "$type": "color"
      },
      "emphasisPlus": {
        "$value": "{base.color.gray.0}",
        "$type": "color"
      }
    },
    "accent": {
      "muted": {
        "$value": "{base.color.blue.4}",
        "$type": "color"
      }
    },
    "success": {
      "muted": {
        "$value": "{base.color.green.4}",
        "$type": "color"
      }
    },
    "attention": {
      "muted": {
        "$value": "{base.color.yellow.4}",
        "$type": "color"
      }
    },
    "severe": {
      "muted": {
        "$value": "{base.color.orange.4}",
        "$type": "color"
      }
    },
    "danger": {
      "muted": {
        "$value": "{base.color.red.4}",
        "$type": "color"
      }
    },
    "done": {
      "muted": {
        "$value": "{base.color.purple.4}",
        "$type": "color"
      }
    },
    "sponsors": {
      "muted": {
        "$value": "{base.color.pink.4}",
        "$type": "color"
      }
    },
    "avatar": {
      "border": {
        "$value": "{base.color.white}",
        "$type": "color",
        "alpha": 0.9
      }
    },
    "diffstat": {
      "deletionBorder": {
        "$value": "{base.color.red.2}",
        "$type": "color"
      },
      "additionBorder": {
        "$value": "{base.color.green.2}",
        "$type": "color"
      }
    },
    "underlinenav": {
      "icon": {
        "$value": "{base.color.gray.1}",
        "$type": "color"
      },
      "borderHover": {
        "$value": "{base.color.gray.3}",
        "$type": "color"
      }
    },
    "topicTag": {
      "border": {
        "$value": "{color.accent.emphasis}",
        "$type": "color"
      }
    },
    "btn": {
      "selectedBg": {
        "$value": "{base.color.gray.6}",
        "$type": "color",
        "alpha": 0.9
      },
      "primary": {
        "text": {
          "$value": "{color.fg.onEmphasis}",
          "$type": "color"
        },
        "border": {
          "$value": "{base.color.green.2}",
          "$type": "color"
        },
        "hover": {
          "bg": {
            "$value": "{base.color.green.3}",
            "$type": "color"
          }
        },
        "hoverBorder": {
          "$value": "{base.color.green.2}",
          "$type": "color"
        },
        "disabledText": {
          "$value": "{color.fg.onEmphasis}",
          "$type": "color",
          "alpha": 0.5
        },
        "disabledBg": {
          "$value": "{base.color.green.5}",
          "$type": "color",
          "alpha": 0.6
        },
        "disabledBorder": {
          "$value": "{base.color.green.2}",
          "$type": "color",
          "alpha": 0.4
        },
        "icon": {
          "$value": "{color.fg.onEmphasis}",
          "$type": "color"
        },
        "counterBg": {
          "$value": "{base.color.black}",
          "$type": "color",
          "alpha": 0.15
        }
      },
      "danger": {
        "hover": {
          "counter": {
            "bg": {
              "$value": "{base.color.black}",
              "$type": "color",
              "alpha": 0.15
            }
          }
        },
        "counterBg": {
          "$value": "{base.color.black}",
          "$type": "color",
          "alpha": 0.15
        }
      }
    },
    "codemirror": {
      "selectionBg": {
        "$value": "{base.color.blue.4}",
        "$type": "color",
        "alpha": 0.4
      }
    },
    "diffBlob": {
      "addition": {
        "fg": {
          "$value": "{color.fg.onEmphasis}",
          "$type": "color"
        }
      },
      "deletion": {
        "fg": {
          "$value": "{color.fg.onEmphasis}",
          "$type": "color"
        }
      },
      "expander": {
        "icon": {
          "$value": "{color.fg.default}",
          "$type": "color"
        }
      },
      "hunk": {
        "numBg": {
          "$value": "{color.accent.muted}",
          "$type": "color",
          "alpha": 0.4
        }
      }
    },

    "actionListItem": {
      "inlineDivider": {
        "$value": "{color.border.muted}",
        "$type": "color"
      },

      "default": {
        "hoverBg": {
          "$value": "{base.color.gray.8}",
          "$type": "color"
        },
        "hoverBorder": {
          "$value": "{color.border.default}",
          "$type": "color"
        },
        "activeBg": {
          "$value": "{base.color.gray.6}",
          "$type": "color"
        },
        "activeBorder": {
          "$value": "{base.color.gray.4}",
          "$type": "color"
        },
        "selectedBg": {
          "$value": "{base.color.gray.6}",
          "$type": "color"
        }
      },
      "danger": {
        "hoverBg": {
          "$value": "{color.danger.emphasis}",
          "$type": "color"
        },
        "activeBg": {
          "$value": "{base.color.red.6}",
          "$type": "color"
        },
        "hoverText": {
          "$value": "{color.fg.onEmphasis}",
          "$type": "color"
        }
      }
    },

    "switchTrack": {
      "border": {
        "$value": "{color.border.default}",
        "$type": "color"
      },
      "checked": {
        "border": {
          "$value": "{base.color.blue.5}",
          "$type": "color"
        }
      }
    }
  }
}
