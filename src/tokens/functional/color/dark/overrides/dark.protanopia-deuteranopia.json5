/**
 * Dark protanopia and deuteranopia (colorblind)
 * @extends all functional tokens
 * @description All overwrites for functional color tokens for dark protanopia and deuteranopia color mode are in this file
 */
{
  fgColor: {
    open: {
      $value: '{base.color.orange.3}',
      $type: 'color',
    },
    closed: {
      $value: '{base.color.gray.3}',
      $type: 'color',
    },
  },
  bgColor: {
    open: {
      muted: {
        $value: '{base.color.orange.4}',
        $type: 'color',
        alpha: 0.4,
      },
      emphasis: {
        $value: '{base.color.orange.5}',
        $type: 'color',
      },
    },
    closed: {
      muted: {
        $value: '{base.color.gray.4}',
        $type: 'color',
        alpha: 0.4,
      },
      emphasis: {
        $value: '{base.color.gray.4}',
        $type: 'color',
      },
    },
  },
  borderColor: {
    open: {
      muted: {
        $value: '{base.color.orange.4}',
        $type: 'color',
        alpha: 0.4,
      },
      emphasis: {
        $value: '{base.color.orange.4}',
        $type: 'color',
      },
    },
    closed: {
      muted: {
        $value: '{base.color.gray.4}',
        $type: 'color',
        alpha: 0.4,
      },
      emphasis: {
        $value: '{base.color.gray.4}',
        $type: 'color',
      },
    },
  },
  button: {
    primary: {
      bgColor: {
        disabled: {
          $value: '{base.color.green.6}',
          $type: 'color',
          mix: null,
        },
      },
    },
  },
}
