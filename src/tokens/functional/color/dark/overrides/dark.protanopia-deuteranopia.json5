/**
 * Dark protanopia and deuteranopia (colorblind)
 * @extends all functional tokens
 * @description All overwrites for functional color tokens for dark protanopia and deuteranopia (colorblind) color mode are in this file
 */
{
  color: {
    open: {
      fg: {
        $value: '{base.color.scale.orange.3}',
        $type: 'color'
      },
      emphasis: {
        $value: '{base.color.scale.orange.5}',
        $type: 'color'
      },
      muted: {
        $value: '{base.color.scale.orange.4}',
        $type: 'color',
        alpha: 0.4
      },
      subtle: {
        $value: '{base.color.scale.orange.4}',
        $type: 'color',
        alpha: 0.15
      }
    },
    closed: {
      fg: {
        $value: '{base.color.scale.gray.3}',
        $type: 'color'
      },

      emphasis: {
        $value: '{base.color.scale.gray.4}',
        $type: 'color'
      },
      muted: {
        $value: '{base.color.scale.gray.4}',
        $type: 'color',
        alpha: 0.4
      },
      subtle: {
        $value: '{base.color.scale.gray.4}',
        $type: 'color',
        alpha: 0.1
      }
    }
  }
}
