        "medium": {
          "$value": {
            "fontWeight": "{text.body.weight}",
            "fontSize": "{text.body.size.medium}",
            "lineHeight": "{text.body.lineHeight.medium}",
            "fontFamily": "{fontStack.sansSerif}"
          },
          "$type": "typography",
          "$description": "Default UI font. Most commonly used for body text."
